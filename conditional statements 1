#include <stdio.h>
int main(void)
{
    int num;
    printf("Enter your mark ");
    scanf("%d",&num);
    if(num >= 85)
	{
        printf(" \nYou got A grade"); // printing outputs
    }
    else if ( num >=70 && num <85) // Note the space between else & if
    {
        printf(" \nYou got B grade");
    }
    else if ( num >=55 && num <70)
	{
        printf(" \nYou got C grade");
    }
    else if ( num >=40 && num<55)
	{
        printf(" \nYou got D grade ");
    }
    else if (num <40)
    {
    	printf ("\nGrade F");
	}
	return 0;
}
